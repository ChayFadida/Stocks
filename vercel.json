{
  "rewrites": [
    { "source": "./src/login", "destination": "/public/html/login.html" },
    { "source": "./src/cool_route", "destination": "/public/html/cool.html" },
    { "source": "/src/api/(.*)", "destination": "/api/graph/$1" },
    { "source": "/(.*)", "destination": "/server.js" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}
